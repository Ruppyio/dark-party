{
  "name": "dark-party",
  "displayName": "Dark Party",
  "description": "âœ¨ VSCode dark theme based on Dracula theme and One Dark Pro color token. For personal use, feel free to download if you want to use it too.",
  "version": "3.1.0",
  "publisher": "ryuukibeat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ruppyio/dark-party"
  },
  "bugs": {
    "url": "https://github.com/Ruppyio/dark-party/issues"
  },
  "engines": {
    "vscode": "^1.41.0",
    "yarn": ">=1.19.0"
  },
  "categories": [
    "Themes"
  ],
  "galleryBanner": {
    "color": "#d55fde",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Dark Party",
        "uiTheme": "vs-dark",
        "path": "./theme/dark-party.json"
      }
    ]
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "scripts": {
    "cm": "cz",
    "check": "node ./src/check-color-theme.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --check .",
    "build": "node ./src/index.js && prettier --write theme/dark-party.json",
    "semantic-release": "semantic-release",
    "postinstall": "husky install"
  },
  "icon": "icon.png",
  "keywords": [
    "dark party",
    "dracula",
    "custom theme",
    "one dark pro",
    "dark",
    "one dark",
    "onedark",
    "atom",
    "color theme",
    "color-theme",
    "color",
    "theme",
    "vscode"
  ],
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/cz-commitlint": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "chroma-js": "^2.1.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "eslint": "^7.32.0",
    "eslint-config-ruppy-node": "^1.7.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "semantic-release": "^17.4.4",
    "semantic-release-vsce": "^3.4.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        1,
        "always",
        100
      ],
      "footer-max-line-length": [
        1,
        "always",
        100
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test",
          "security"
        ]
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
